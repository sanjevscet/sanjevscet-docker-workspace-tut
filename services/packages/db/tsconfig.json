{
  "compilerOptions": {
    "experimentalDecorators": true,
    "composite": true,
    "target": "ES2016",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strictPropertyInitialization":false,
    "emitDecoratorMetadata": true,
  }
}
